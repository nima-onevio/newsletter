/* Navigation
=======================*/
nav {
    position:relative;
}
#navigation {
    background: url({{ 'navBgWhite.png' | asset_url }})  repeat-x;
    height: 26px;
    line-height: 26px;
}
ul.nav {
    font-family: "montserratregular";
    margin-bottom: 0;
    list-style: none;
    text-align: center;
}
ul.nav li {
    display: inline-block;
    position: relative;
    z-index: 88000;
    width: 104px;
    border-right: 1px solid #E6E7E8;
}
ul.nav li a {
    display: block;
    padding: 0 20px;
    color: #414041;
    font-size: 1em;
    line-height: 25px;
    font-weight: normal;
}
ul.nav li a:hover {
    color: #6FBE46;
    text-decoration: none;
}
ul.nav > li a:hover + .navBorder,
ul.nav > li.active .navBorder {
    display: block;
    height: 2px;
    width: 108px;
    position: absolute;
    top: 23px;
    background-color: #6FBE46;
    left: -3px;
}
ul.nav .promote {
    background: url({{ 'navHover.png' | asset_url }}) right center no-repeat;
}
/*---sub nav---*/
ul.nav li ul {
    display: none;
    position: absolute;
    top: 24px;
    z-index: -1;
    border: 1px solid #E6E7E8;
    border-radius: 4px;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    width: 107px;
    left: -4px;
    margin: 0;
    background: white;
}
ul.nav li:hover ul {
    display: block;
}
ul.nav li ul li {
    width: 107px;
    text-align: left;
    line-height: 30px;
    margin: 0;
    padding: 0;
    background-color: white;
    border-bottom: 1px solid #E6E7E8;
}
ul.nav li ul li a {
    text-align: left;
    font-size: 11px;
    font-weight: normal;
    line-height: 29px;
}
ul.nav li ul li:hover {
    color: #6FBE46;
    text-decoration: none;
    cursor: pointer;
}
ul.nav li ul li a:hover,
ul.nav .navIco:hover + a{
    color: #6FBE46;
    text-decoration: none;
    background: url({{ 'navHover.png' | asset_url }}) 105px top no-repeat;
}
ul.nav li ul li.first a:hover{
    -webkit-border-top-left-radius: 4px;
    -webkit-border-top-right-radius: 4px;
    -moz-border-radius-topleft: 4px;
    -moz-border-radius-topright: 4px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}
ul.nav li ul li.last a:hover {
    -webkit-border-bottom-right-radius: 4px;
    -webkit-border-bottom-left-radius: 4px;
    -moz-border-radius-bottomright: 4px;
    -moz-border-radius-bottomleft: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}
ul.nav li ul li:last-child {
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom: none;
}
/*----first nav icon----*/
ul.nav > li:first-child {
    min-width: 40px;
    max-width: 40px;
}
ul.nav > li:first-child {
    text-align: right;
    border-left:1px solid #E6E7E8;
    background: url({{ 'icoOnevioNav.png' | asset_url }})  15px center no-repeat;
}
ul.nav > li:first-child > a,
ul.nav > li:first-child > a:hover,
ul.nav > li:first-child > a:visited {
    color: transparent;
}
ul.nav > li:first-child a:hover + .navBorder,
ul.nav > li.active:first-child .navBorder{
    width: 41px;
    left: 0;
}
/*--first nav icon subnav--*/
ul.nav .navIco {
    display: block;
    position: absolute;
    left: 0;
    width: 39px;
    height: 30px;
    border-right: 1px solid #E6E7E8;
}
ul.nav > li:first-child ul,
ul.nav > li:first-child ul li {
    width: 149px;
}
ul.nav > li:first-child ul li a {
    margin-left: 30px;
}
