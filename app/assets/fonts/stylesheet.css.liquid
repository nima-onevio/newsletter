}

/* =============== */
/* = Footer = */
/* =============== */

#footer {
background: {{ settings.footer-background }};
padding-bottom: 20px;
    }
footer.main {
    position: fixed;
    bottom: 0;
    width: 100%;
}
footer.main .container {
    background: white;
}
footer.main .downArrow {
    width: 44px;
    height:14px;
    display: block;
    background: url({{ 'downArrow.png' | asset_url}}) center center no-repeat;
    margin: 0 auto;
    padding: 20px 0 35px 0;
}
footer.main .share {
    width: 132px;
    height: 48px;
    display: block;
    line-height: 62px;
    margin: 0 auto;
    outline:none;
    color: white;
    text-align: center;
    background: url({{ 'shareBg.png' | asset_url}}) center center no-repeat;
}
footer.main .share:hover,
footer.main .share:visited{
    text-decoration: none;
}
.left {
    float: left;
}
.left h4 {
    display: inline;
    margin-right: 10px;
}
.right {
    float: right;
}
ul.list {
    list-style-type: none;
}
ul.list h4 {
    display: inline-block;
}
ul.list li {
    display: inline-block;
}
ul#big-footer {
border-top: 1px solid {{ settings.footer-top-border }};
margin-bottom: 20px; border-bottom: 1px solid {{ settings.footer-bottom-border }};
padding: 30px 0 10px;
list-style-type: none;
    }
ul#big-footer li {
    margin-bottom: 10px;
}
ul#big-footer li img {
    width: 25px;
    margin: 0 5px 5px 0;
}
ul#big-footer {
    list-style-type: none;
color: {{ settings.footer-text-color }};
    }
ul#big-footer a {
color: {{ settings.footer-text-color }};
    }
ul#big-footer h4 {
    font-weight: bold! important;
    margin-bottom: 10px;
color: {{ settings.footer-header-color }};
    }
#close img {
    width: 50px;
    margin: 0 10px 10px 0;
}

#close-right { text-align: right;  line-height: 32px; color: {{ settings.footer-text-color }}; }

